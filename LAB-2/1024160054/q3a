#include <iostream>
using namespace std;

int findMissingLinear(int arr[], int n) {
    for (int i = 0; i < n - 1; i++) {
        if (arr[i] != i + 1) {
            return i + 1;  // Expected number at this position
        }
    }
    return n;  // If last number is missing
}

int main() {
    int arr[] = {1, 2, 3, 5, 6};  // Missing 4
    int n = 6;  // Full range should be 1 to 6

    int missing = findMissingLinear(arr, n);
    cout << "Missing number (Linear Search): " << missing << endl;

    return 0;
}
