#include <iostream>
#include <unordered_set>
using namespace std;

int main() {
    int n;
    cout << "Enter array length: ";
    cin >> n;
    int arr[n];
    cout << "Enter elements:\n";
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
    }

    unordered_set<int> distinctElements;
    for (int i = 0; i < n; i++) {
        distinctElements.insert(arr[i]);
    }

    cout << "Number of distinct elements: " << distinctElements.size() << "\n";
    return 0;
}
